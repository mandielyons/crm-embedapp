											ConnectionID="connCRMData"
											ID="dlTotalAcresByYear"
											Source="SELECT CropYear, SUM(Acres) AS TotalAcres
FROM mkcCropForecastAcres
WHERE ProdRptCatDesc = &apos;Seed&apos; And AccountId = &apos;@Request.CRMAccount~&apos;
