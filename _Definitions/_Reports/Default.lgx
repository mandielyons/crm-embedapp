										OptionTooltipColumn="Select a ticket range"
										OptionValueColumn="CropName"
										>
										<Note
											Note="&lt;!--Get appropriate data for this select--&gt;"
										/>
										<DataLayer
											ConnectionID="connCRMData"
											ID="dlCropName"
											Source="Select Distinct mkcCropForecastAcres.CropName
From mkcCropForecastAcres 
Order By CropName ASC"
											Type="SQL"
										/>
										<Validation
											Class="ThemeErrorText"
											Type="Required"
										/>
									</InputSelectList>
									<LineBreak
										LineCount="2"
									/>
								</ResponsiveColumn>
								<ResponsiveColumn/>
							</ResponsiveRow>
							<ResponsiveRow>
								<ResponsiveColumn
									ColspanExtraSmallScreen="4"
									ColspanLargeScreen="3"
									ColspanMediumScreen="3"
									ColspanSmallScreen="4"
									>
									<Label
										Caption="Crop Type: "
										Class="bold"
									/>
									<LineBreak
										LineCount="2"
									/>
								</ResponsiveColumn>
								<ResponsiveColumn
									ColspanExtraSmallScreen="4"
									ColspanLargeScreen="3"
									ColspanMediumScreen="3"
									ColspanSmallScreen="4"
									>
									<InputSelectList
										DefaultValue="@Request.CropType~"
										ID="selCropType"
										IncludeBlank="False"
										IncludeBlankCaption="Select a crop type"
										OptionCaptionColumn="CropType"
										OptionTooltipColumn="Select a ticket range"
										OptionValueColumn="CropType"
										>
										<Note
											Note="&lt;!--Get appropriate data for this select--&gt;"
										/>
										<DataLayer
											ConnectionID="connCRMData"
											ID="dlCropType"
											Source="Select Distinct mkcCropForecastAcres.CropType
From mkcCropForecastAcres 
Order By CropType ASC"
											Type="SQL"
										/>
										<Validation
											Class="ThemeErrorText"
											Type="Required"
										/>
									</InputSelectList>
									<LineBreak
										LineCount="2"
									/>
								</ResponsiveColumn>
								<ResponsiveColumn/>
							</ResponsiveRow>
							<ResponsiveRow>
								<ResponsiveColumn
									Class="ThemeAlignLeft"
									ColspanExtraSmallScreen="6"
									ColspanLargeScreen="3"
									ColspanMediumScreen="3"
									ColspanSmallScreen="6"
									>
									<Label
										Caption="Crop Acres: "
										Class="bold"
									/>
									<LineBreak
										LineCount="2"
									/>
								</ResponsiveColumn>
								<ResponsiveColumn
									ColspanExtraSmallScreen="4"
									ColspanLargeScreen="3"
									ColspanMediumScreen="3"
									ColspanSmallScreen="4"
									>
									<InputText
										Class="ThemeAlignRight"
										DefaultValue="@Request.Acres~"
										Format="General Number"
										ID="iptAcres"
										Tooltip="Enter the number of acres"
										>
										<Validation
											Class="ThemeErrorText"
											Type="Required"
										/>
									</InputText>
									<LineBreak
										LineCount="2"
									/>
								</ResponsiveColumn>
								<ResponsiveColumn/>
							</ResponsiveRow>
							<ResponsiveRow>
								<ResponsiveColumn
									Class="ThemeAlignLeft"
									ColspanExtraSmallScreen="12"
									ColspanLargeScreen="12"
									ColspanMediumScreen="12"
									ColspanSmallScreen="12"
									>
									<InputText
										Class="ThemeHidden"
										DefaultValue="@Request.CRMAccount~"
										ID="iptAccountId"
									/>
									<Button
										Caption="Save"
										ID="btnAdd"
										>
										<Note
											Note="&lt;!--Add Action.Process here--&gt;"
										/>
										<Action
											ID="procAdd"
											Process="SQLTasks"
											TaskID="insertRow"
											Type="Process"
											>
											<WaitPage/>
											<LinkParams/>
										</Action>
									</Button>
									<Spaces
										Size="15"
									/>
									<Button
										Caption="Cancel"
										ID="btnCancel"
										>
										<Remark>
											<Action
												ConfirmMessage="Changes will be lost. Continue without saving?"
												ID="refreshReport"
												Type="Report"
												Validate="True"
												>
												<Target
													Report="Default"
													Type="Report"
													>
													<WaitPage/>
												</Target>
											</Action>
										</Remark>
										<Action
											Display="Hide"
											ElementID="panelAddAcres"
											ID="hideAddPanel"
											Type="ShowElement"
										/>
									</Button>
								</ResponsiveColumn>
							</ResponsiveRow>
						</Division>
					</Division>
				</ResponsiveColumn>
			</ResponsiveRow>
		</PopupPanel>
		<ResponsiveRow>
			<ResponsiveColumn>
				<Division
