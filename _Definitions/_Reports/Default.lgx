<?xml version="1.0" encoding="utf-8"?>
<Report
	ID="Default"
	>
	<DefaultRequestParams
		CRMAccount=""
		Maint=""
		StatusMsg=""
	/>
	<LocalData>
		<DataLayer
			ConnectionID="connCRMData"
			ID="dlCustomerBusiness"
			Source="Select mkcCropForecastAcres.ProdRptCatDesc,
  mkcCropForecastAcres.CropYear,
  mkcCropForecastAcres.Acres,
  mkcCropForecastAcres.AccountId
From mkcCropForecastAcres
Where mkcCropForecastAcres.CropYear = Year(GetDate()) And
  mkcCropForecastAcres.AccountId = &apos;@Request.CRMAccount~&apos;"
			Type="SQL"
			>
			<CrosstabFilter
				CrosstabColumn="ProdRptCatDesc"
				CrosstabLabelColumn="CropYear"
				CrosstabValueColumn="Acres"
				CrosstabValueFunction="Sum"
			/>
			<CalculatedColumn
				Formula="Year(&quot;@Function.Date~&quot;)"
				ID="cYear"
			/>
			<CalculatedColumn
				Formula="Month(&quot;@Function.Date~&quot;)"
				ID="cMonth"
			/>
		</DataLayer>
	</LocalData>
	<Body>
