								<ResponsiveColumn
									Class="ThemeAlignLeft"
									ColspanExtraSmallScreen="4"
									ColspanLargeScreen="3"
									ColspanMediumScreen="3"
									ColspanSmallScreen="4"
									>
									<Label
										Caption="Crop Year: "
										Class="bold"
									/>
									<LineBreak
										LineCount="2"
									/>
								</ResponsiveColumn>
								<ResponsiveColumn
									ColspanExtraSmallScreen="4"
									ColspanLargeScreen="3"
									ColspanMediumScreen="3"
									ColspanSmallScreen="4"
									>
									<InputSelectList
										ID="newCropYear"
										OptionCaptionColumn="CropYear"
										OptionValueColumn="CropYear"
										>
										<DataLayer
											ConnectionID="connCRMData"
											ID="dlCropYear"
											Source="Select Distinct mkcCropForecastAcres.CropYear
From mkcCropForecastAcres 
Order By CropYear DESC"
											Type="SQL"
											>
											<ConditionFilter
												Condition="@Data.CropYear~ &gt; @Data.CropYear~ - 5 "
											/>
										</DataLayer>
									</InputSelectList>
									<LineBreak
										LineCount="2"
									/>
								</ResponsiveColumn>
								<ResponsiveColumn/>
							</ResponsiveRow>
							<ResponsiveRow>
								<ResponsiveColumn
									ColspanExtraSmallScreen="4"
									ColspanLargeScreen="3"
									ColspanMediumScreen="3"
									ColspanSmallScreen="4"
									>
									<Label
										Caption="Crop Name: "
										Class="bold"
									/>
									<LineBreak
										LineCount="2"
									/>
								</ResponsiveColumn>
								<ResponsiveColumn
									ColspanExtraSmallScreen="4"
									ColspanLargeScreen="3"
									ColspanMediumScreen="3"
									ColspanSmallScreen="4"
									>
									<InputSelectList
										DefaultValue="@Request.CropName~"
										ID="selCropName"
